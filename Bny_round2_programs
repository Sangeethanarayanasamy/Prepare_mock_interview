WebElement abc=wait.until(ExpectedConditions.elementToBeClickable(By.id("username"));

Wait<WebDriver> wait=new FluentWait<>(driver)
.withTimeout(Duratiomn.ofSeconds(10))
.pollingEvery(Duration.ofSeconds(1))
.ignoring(NoSuchElementException.class);

