// Online Java Compiler
// Use this editor to write, compile and run your Java code online

class Main {
    public static void main(String[] args) {
       int rows=4;
       for(int i=1;i<=rows;i++){
           for(int j=1;j<rows;j++){
               System.out.print(" ");
           }
           for(int j=i;j<2*i;j++){
               System.out.print(j+" ");
           }
           for(int j=2*i-2;j>=i;j--){
               System.out.print(j+" ");
           }
           System.out.println(" ");
       }
       
    }
        
    
}

